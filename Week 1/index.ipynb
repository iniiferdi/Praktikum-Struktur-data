    "import array as arr\n",
    "\n",
    "arr = arr.array('i', [1,2,3,4,5])\n",
    "\n",
    "print('Array before insertion', end=': ')\n",
    "\n",
    "for i in range(0, 5):\n",
    "    print(arr[i], end=' | ')\n",
    "    \n",
    "print()\n",
    "    \n",
    "arr.insert(1, 2)\n",
    "\n",
    "print('Array after insertion', end=': ')\n",
    "\n",
    "for i in (arr):\n",
    "    print(i, end=' | ')"
